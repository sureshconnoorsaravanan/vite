{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:prod": "cross-env NODE_ENV=production vite build --mode production",
    "build:staging": "cross-env NODE_ENV=production vite build --mode staging",
    "serve:dev": "npm run build:dev && cross-env NODE_ENV=development vite preview --mode development",
    "serve:prod": "npm run build:prod && cross-env NODE_ENV=production vite preview --mode production",
    "serve:staging": "npm run build:staging && cross-env NODE_ENV=production vite preview --mode staging"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react-redux": "^7.1.34",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "styled-components": "^6.1.13",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "i18next-localstorage-backend": "^4.2.0",
    "react-i18next": "^15.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.13.0",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
